<div class="flexrow">
	<fieldset>
		{{log this}}
		{{log system.schema.proficiencies}}
		<legend>{{localize system.schema.fields.proficiencies.label}}</legend>
		{{formInput
			system.schema.fields.proficiencies
			value=source.proficiencies
		}}
		<div class="form-group">
			<div class="form-fields">
				<input type="text"
					name=""
					value="">
				<button type="button"
					class="flex0"
					data-action="create"
					data-field-path="system.proficiencies"
					data-type="proficiency">
					{{{SYSTEM.icons.create}}}
				</button>
			</div>
		</div>
	</fieldset>
</div>